<script>
    function addURL(form) {
        let packet = {
            name: form.name.value,
            url: form.url.value,
        }
        console.log(packet)
        fetch('/r', {
            method: "POST",
            body: JSON.stringify(packet)
        })
            .then(alert("Added url"))
    }
</script>
<form name="redirect" action="", method="POST">
    <input type="text" name="name" value="">
    <input type="text" name="url" value="">
    <input type="button" name="button" value="Add redirect" onclick="addURL(this.form)">
</form>